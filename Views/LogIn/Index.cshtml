@model   PersonnelManager.Web.Infrastructure.ViewModels.LogIn.LogInViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/LayOut/LayOut.cshtml";
}
<script>
    $(function ()
    {
        document.getElementById("NavBar").style.display = "none";
    });
</script>

<h2 class="muted"> User Login </h2>
<div>

    

    <div>
        <input  class="input-large input-block-level " data-bind='value:Email'  placeholder="Email" type="email"/>     
                             
    </div>
    <div>
        <input   class="input-large input-block-level " data-bind='value:Password'  placeholder="Password" type="password" />  
                             
    </div>

    <button id="loading" class="btn btn-success" data-bind="click:logIn">
        <i class="icon-user icon-white"></i> Login
    </button>
   
</div>

<script src="~/Scripts/LogInJs/LogIn.js"></script>
