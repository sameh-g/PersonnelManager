<div id="myModalQualification" class="modal hide fade">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h3>Qualification Skill</h3>
        </div>
    <div class="modal-body">
        <p id="Qualification">
            <table class="table table-hover">
                <tbody>
                    <td>
                        <label> Type Of Qualification</label>
                        <select class="dropdown" data-bind="options: QualificationTypes, value: qualification().FKQualificationType, optionsValue: 'Id', optionsText: 'TypeOfQualification1'"></select>
                        <label>Note</label>
                        <textarea class="input-large  input-block-level "  placeholder="Note" data-bind="value:qualification().Notes"></textarea>
                    </td>
                    <td>
                       <label>  Qualification subject</label>
                       <select class="dropdown" data-bind="options: QualificationSubjects, value: qualification().FKSubject, optionsValue: 'Id', optionsText: 'Subject'"></select>
                    </td>
                </tbody>
              </table>
            </p>
        </div>
        <div class="modal-footer">
            <a href="#" class="btn" data-bind="click:CloseQualification" >Close</a>
            <a href="#" class="btn btn-primary" data-bind="click:AddEditeNewQuallification">Ok</a>
        </div>
    </div>